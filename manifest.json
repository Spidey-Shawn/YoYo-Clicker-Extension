{
  "manifest_version": 3,
  "name": "YoYo Clicker",
  "version": "1.3",
  "description": "Track points while watching videos using trackpad/mouse gestures",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
